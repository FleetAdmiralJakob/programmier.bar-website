{
  "kind": "singleType",
  "collectionName": "meetup_pages",
  "info": {
    "name": "Meetup",
    "description": "Der Inhalt der \"Meetup\"-Seite unserer Website."
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "meta_title": {
      "type": "string",
      "required": true,
      "maxLength": 42
    },
    "meta_description": {
      "type": "text",
      "maxLength": 160,
      "required": true
    },
    "cover_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": ["images"],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "intro_heading": {
      "type": "string",
      "required": true
    },
    "intro_text_1": {
      "type": "richtext",
      "required": true
    },
    "intro_text_2": {
      "type": "richtext",
      "required": true
    },
    "corona_text": {
      "type": "richtext",
      "required": true
    },
    "meetups_heading": {
      "type": "string",
      "required": true
    }
  }
}
